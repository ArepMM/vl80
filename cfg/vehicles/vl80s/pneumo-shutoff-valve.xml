<?xml version="1.0" encoding="UTF-8"?>
<Config>
	<Device>
		<Order>1</Order>
		<SubStepNum>1</SubStepNum>
		<Solver>euler</Solver>

		<!-- Начальное состояние разобщительного крана: 0 - закрыт, 1 - открыт -->
		<IsOpened>1</IsOpened>

		<!-- Условный объём труб между разобщительным краном и оборудованием, м^3 -->
		<Vc>0.001</Vc>

		<!-- Коэффициент выпуска воздуха из оборудования через атмосферное отверстие при закрытом разобщительном кране -->
		<kAtm>0.1</kAtm>

		<!-- Коэффициент громкости озвучки к потоку выпуска воздуха через атмосферное отверстие -->
		<K_sound>2.5</K_sound>
	</Device>
</Config>