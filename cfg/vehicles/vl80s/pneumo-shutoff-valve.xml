<?xml version="1.0" encoding="UTF-8"?>
<Config>
	<Device>
		<Order>2</Order>
		<SubStepNum>1</SubStepNum>
		<Solver>euler</Solver>

		<!-- Начальное состояние разобщительного крана: 0 - закрыт, 1 - открыт -->
		<IsOpened>1</IsOpened>

		<!-- Время переключения концевого крана, с -->
		<SwitchTime>0.2</SwitchTime>

		<!-- Коэффициент разрядки из рукава в атмосферу при закрытом концевом кране -->
		<kAtm>0.1</kAtm>

		<!-- Коэффициент громкости озвучки к потоку опорожнения рукава через атмосферное отверстие -->
		<K_sound>2.5</K_sound>
	</Device>
</Config>