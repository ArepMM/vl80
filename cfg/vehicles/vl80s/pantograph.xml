<?xml version="1.0" encoding="UTF-8"?>
<Config>
	<Device>
		<Order>2</Order>
		<SubStepNum>1</SubStepNum>
		<Solver>euler</Solver>

		<!-- Объём цилиндра пневмопривода токоприёмника, м^3 -->
		<Vc>0.01</Vc>

		<!-- Коэффициент перетока воздуха из магистрали в цилиндр пневмопривода токоприёмника -->
		<Kc>0.02</Kc>

		<!-- Минимальное давление срабатывания пневмопривода токоприёмника, МПа -->
		<Pmin>0.35</Pmin>

		<!-- Номинальное давление пневмопривода токоприёмника, МПа -->
		<Pnom>0.5</Pnom>

		<!-- Время поднятия токоприёмника до максимальной высоты при номинальном давлении / опускания при нулевом давлении, с -->
		<MotionTime>3.0</MotionTime>

		<!-- Высота полоза в опущенном состоянии, м -->
		<HeightDown>5.100</HeightDown>

		<!-- Максимальная высота полоза в поднятом состоянии, м -->
		<HeightUp>7.000</HeightUp>
	</Device>
</Config>
