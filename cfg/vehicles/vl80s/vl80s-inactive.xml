<?xml version="1.0" encoding="UTF-8"?>
<Config>
	<Vehicle>
		<EmptyMass>96000</EmptyMass>
		<PayloadMass>800</PayloadMass>
		<Length>21.0</Length>
		<!--<Length>16.42</Length>-->
		<WheelDiameter>1.25</WheelDiameter>
		<MainResist>loco-resist</MainResist>
		<WheelRailFriction>loco-AC</WheelRailFriction>
		<NumAxis>4</NumAxis>
		<WheelInertia>100.0</WheelInertia>

		<!-- Натягиваем вл60к -->
		<SoundDir>vl60</SoundDir>
		<AnimationsConfigDir>vl60k</AnimationsConfigDir>

		<ExtModelName>VL60k-1737/VL60k-1737.osgt</ExtModelName>
		<!--<ModelShift>0.0 -2.29 0.312</ModelShift>-->
		<ModelShift>0.0 0.0 0.312</ModelShift>

		<CabineModel>VL60k-1737/VL60k-cabview.osgt</CabineModel>
		<!--<CabineShift>0.0 -2.29 0.317</CabineShift>-->
		<CabineShift>0.0 0.0 0.317</CabineShift>

		<!--<DriverPos>0.8 6.1 3.6</DriverPos>-->
		<DriverPos>0.8 8.4 3.6</DriverPos>
	</Vehicle>


	<Settings>
		<!-- Передаточное число зубчатой передачи тяговых электродвигателей -->
		<ReductorCoeff>4.19</ReductorCoeff>

		<!-- Модуль автосцепного устройства -->
		<CouplingModule>sa3</CouplingModule>
		<!-- Конфиг автосцепного устройства -->
		<CouplingConfig>sa3</CouplingConfig>

		<!-- Состояние реверсивной рукоятки -->
		<!-- (1 - вставлена, 0 - извлечена) -->
		<ReversHandleState>0</ReversHandleState>

		<!-- Состояние устройства блокировки тормозов -->
		<!-- (1 - разблокирована, 0 - заблокирована) -->
		<BrakeLockState>0</BrakeLockState>

		<!-- Положение комбинированного крана -->
		<!-- ( -1 - положение двойной тяги, 0 - поездное, 1 - экстренное торможение) -->
		<CombineCranePos>0</CombineCranePos>

		<!-- Положение разобщительного крана воздухораспределителя и тормозной магистрали КН8 -->
		<!-- (0 - закрыт, 1 - открыт) -->
		<ShutoffValveADBPpos>1</ShutoffValveADBPpos>
		<!-- Положение разобщительного крана воздухораспределителя и импульсной магистрали КН20 -->
		<!-- (0 - закрыт, 1 - открыт) -->
		<ShutoffValveADILpos>1</ShutoffValveADILpos>

		<!-- Модуль поездного крана машиниста -->
		<BrakeCraneModule>krm395</BrakeCraneModule>
		<!-- Конфиг поездного крана машиниста -->
		<BrakeCraneConfig>krm395</BrakeCraneConfig>
		<!-- Положение рукоятки поездного крана машиниста -->
		<!-- (1 - I Отпуск, 2 - II Поездное, 3 - III Перекрыша, 4 - IV Перекрыша с питанием, -->
		<!--  5 - Va Торможение замедленным темпом, 6 - V Служебное торможение, 7 - VI Экстренное торможение) -->
		<BrakeCraneInitPos>7</BrakeCraneInitPos>

		<!-- Модуль крана вспомогательного тормоза -->
		<LocoCraneModule>kvt254</LocoCraneModule>
		<!-- Конфиг крана вспомогательного тормоза -->
		<LocoCraneConfig>kvt254</LocoCraneConfig>
		<!-- Положение рукоятки крана вспомогательного тормоза -->
		<!-- (от 0.0 - тормоз отпущен  до 1.0 - тормоз применён полностью -->
		<LocoCraneInitPos>1.0</LocoCraneInitPos>

		<!-- Модуль воздухораспределителя -->
		<AirDistModule>vr483</AirDistModule>
		<!-- Конфиг воздухораспределителя -->
		<AirDistConfig>vr483</AirDistConfig>

		<!-- Объём главных резервуаров, м^3 -->
		<MainReservoirVolume>0.9</MainReservoirVolume>
		<!-- Темп утечек из главных резервуаров -->
		<MainReservoirFlow>1.0e-6</MainReservoirFlow>

		<!-- Объём запасного резервуара, м^3 -->
		<SupplyReservoirVolume>0.055</SupplyReservoirVolume>
		<!-- Темп утечек из запасного резервуара -->
		<SupplyReservoirFlow>1.0e-6</SupplyReservoirFlow>

		<!-- Объём резервуара токоприёмника, м^3 -->
		<PantReservoirVolume>0.150</PantReservoirVolume>
		<!-- Темп утечек из резервуара токоприёмника -->
		<PantReservoirFlow>1.0e-6</PantReservoirFlow>

		<!-- Объём резервуара главного выключателя, м^3 -->
		<SwitchReservoirVolume>0.032</SwitchReservoirVolume>
		<!-- Темп утечек из резервуара главного выключателя -->
		<SwitchReservoirFlow>1.0e-6</SwitchReservoirFlow>
	</Settings>
</Config>