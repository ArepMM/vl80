<?xml version="1.0" encoding="UTF-8"?>
<Config>
	<Device>
		<Order>2</Order>
		<SubStepNum>1</SubStepNum>
		<Solver>euler</Solver>

		<!-- Номинальное напряжение катушки электромагнитного пневматического вентиля, В -->
		<U_nom>50.0</U_nom>

		<!-- Сопротивление катушки электромагнитного пневматического вентиля, Ом -->
		<R>170.0</R>

		<!-- Постоянная времени обмотки, с -->
		<T>0.1</T>

		<!-- Максимальный уровень отключения (0.0 - 1.0) -->
		<level_off>0.6</level_off>
		<!-- Минимальный уровень включения (0.0 - 1.0) -->
		<level_on>0.7</level_on>

		<!-- Условный объём труб между разобщительным краном и оборудованием, м^3 -->
		<Vc>0.001</Vc>

		<!-- Коэффициент выпуска воздуха из оборудования через атмосферное отверстие при отключенном вентиле -->
		<kAtm>0.1</kAtm>

		<!-- Коэффициент громкости озвучки к потоку выпуска воздуха через атмосферное отверстие -->
		<K_sound>2.5</K_sound>
	</Device>
</Config>