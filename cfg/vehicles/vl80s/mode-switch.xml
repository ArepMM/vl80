<?xml version="1.0" encoding="UTF-8"?>
<Config>
	<Device>

		<Order>2</Order>
		<SubStepNum>1</SubStepNum>
		<Solver>euler</Solver>

		<!-- Объём цилиндра пневмопривода, м^3 -->
		<Vc>0.01</Vc>

		<!-- Коэффициент перетока воздуха из магистрали в цилиндр пневмопривода -->
		<Kc>0.01</Kc>

		<!-- Минимальное давление срабатывания пневмопривода, МПа -->
		<Pmin>0.35</Pmin>

		<!-- Номинальное давление пневмопривода токоприёмника, МПа -->
		<Pnom>0.5</Pnom>

		<!-- Время переключения, с -->
		<MotionTime>1.0</MotionTime>

	</Device>
</Config>
